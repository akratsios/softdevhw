def timer(f):
    def inner(*arg);
        t = time.


@timer
@name_log
def quicksort(g):
    if len(g) <= 1:
        return g
    pivot = random.choice
    lower = [x for x in g if x < pivot]
    upper = [x for x in g if x > pivot]
    return quicksort(lower) + ([pivot] * g.count(pivot)) + quicksort(upper)

x = randList(10, -10, 10)
q = quicksort(x)
name_log(quicksort)
